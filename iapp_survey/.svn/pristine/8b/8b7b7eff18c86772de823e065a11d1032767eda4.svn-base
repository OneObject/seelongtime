<%@page contentType="text/html; charset=utf-8" %>
<%@include file="/WEB-INF/jsp/include/admin/taglibs.jsp" %>
<!DOCTYPE HTML>
<html>
    <head>
        <title>【调研】创新e校园，新行员培训调研活动开始啦！</title>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0;" name="viewport">
        <meta content="yes" name="apple-mobile-web-app-capable">
        <meta content="black" name="apple-mobile-web-app-status-bar-style">
        <meta content="telephone=no" name="format-detection">
        <link href="ixin/css/Site.css" rel="stylesheet" type="text/css">
        <link href="ixin/css/style.css" rel="stylesheet" type="text/css">
    </head>
    <body>
        <div class="wrap" id="wrap" style="">
            <div id="head">
            </div>
            <article class="post">
                <h1 class="post-title" id="title">【调研】创新e校园，新行员培训调研活动开始啦！</h1>
                <div class="post-desc" id="votedate">
                    投票时间：2013-08-01~2015-08-31
                </div>
            </article>
            <div id="inner">
                <h3 class="tpicon"><span>投票</span><span class="tpnum fn-right" id="count">49人参与投票</span></h3>
                <div id="item">
                     <dl class="askmodel" id="timu1">
                        <dt class="ask-problem">
                            <span class="ask-title">1.请选出你心目中最受欢迎的讲师</span>
                            <span class="ask-des">（多选）</span>
                        </dt>
                        <dd class="ask-item" id="mg1">
                            <div class="ask-img">
                                <img src="img/nv3.jpg" alt="xx">
                            </div>
                            <div>
                                <label class="ask-checkbox">
                                </label>
                                <span class="ask-option">01.王亚莎教授</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="mg2">
                            <div class="ask-img">
                                <img src="img/nv2.jpg" alt="xx">
                            </div>
                            <div>
                                <label class="ask-checkbox">
                                </label>
                                <span class="ask-option">02.王安麟教授</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="mg3">
                            <div class="ask-img">
                                <img src="img/nv1.jpg" alt="xx">
                            </div>
                            <div>
                                <label class="ask-checkbox">
                                </label>
                                <span class="ask-option">03.史林启教授</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="mg4">
                            <div class="ask-img">
                                <img src="img/nv4.jpg" alt="xx">
                            </div>
                            <div>
                                <label class="ask-checkbox">
                                </label>
                                <span class="ask-option">04.王立群教授</span>
                            </div>
                        </dd>
                    </dl>
                    <dl class="askmodel" style="" id="timu2">
                        <dt class="ask-problem">
                            <span class="ask-title">2.请选出你心目中最感兴趣的课程内容</span><span class="ask-des">（多选）</span>
                        </dt>
                       <dd class="ask-item" id="q1">
                            <div>
                                <label class="ask-checkbox">
                                </label>
                                <span class="ask-option">01.职场礼仪</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="q2">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">02.职业素养</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="q3">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">03.金融知识</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="q4">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">04.操作合规</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="q5">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">05.其他</span>
                            </div>
                        </dd>
                    </dl>
                    <dl class="askmodel" style="" id="timu3">
                        <dt class="ask-problem">
                            <span class="ask-title">3.请选出你最常用的学习设备。</span><span class="ask-des">（单选）</span>
                        </dt>
                         <dd class="ask-item" id="dq1">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">01.iPad</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="dq2">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">02.iPhone</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="dq3">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">03.微信</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="dq4">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">04.PC电脑</span>
                            </div>
                        </dd>
                        <dd class="ask-item" id="dq5">
                            <div>
                                <label class="ask-checkbox">
                                </label><span class="ask-option">05.其他</span>
                            </div>
                        </dd>
                    </dl>
                   
                </div>
                <div id="J_load" style="display: none;" class="tc">
                    <img src="ixin/img/load.gif" width="27px">
                </div>
                <div class="tc">
                    <a style="" href="javascript:;" id="submit" class="btnGreen">提交</a>
                </div>
            </div>
        </div>
        <script type="text/javascript" src="ixin/js/jquery-1.10.2.min.js"></script>
        <script type="text/javascript" src="ixin/js/zepto.min.js"></script>
        <script src="ixin/js/selector.js" type="text/javascript"></script>
        <script type="text/javascript" charset="utf-8">
            $(document).ready(function(){
                $("#timu1,#timu2").find(".ask-item").each(function(){
                    $(this).bind("click", function(){
                        if($(this).attr("class") == "ask-item active"){
                            $(this).removeClass("active");
                        }else{
                            $(this).addClass("active");
                        }
                       
                    });
                });
                $("#timu3").find(".ask-item").each(function(){
                    $(this).bind("click", function(){
                        remove_style("timu3");
                       $(this).addClass("active");
                    });
                });
            });
            //单选
            function remove_style(id){
                $("#"+id).find(".active").each(function(){
                    $(this).removeClass("active");
                });
            }
            $("#submit").click(function(){
                var anw = [];
                $("#item").find(".active").each(function(){
                    anw.push($(this).attr("id")+",");
                });
                self.location.href="result.html?anw="+anw;
            });
        </script>
    </body>
</html>
