package com.longtime.app.ixin.mgr.model;

/**
 * 调查、投票的问题类  对应表 ixin_question
 * 
 * @author Administrator
 *
 */
public class QuestionEntity {

	private String id;//主键
	private int type;//单选、多选
	private String title;//问题的标题
	private String voteOrSurveyId;//投票，调查实体的ID 例如：SurveyEntity的ID
	private int sortNum;//题号
	private int maxOption;//最多可以选择的题目数量
	
	public int getMaxOption() {
		return maxOption;
	}
	public void setMaxOption(int maxOption) {
		this.maxOption = maxOption;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getVoteOrSurveyId() {
		return voteOrSurveyId;
	}
	public void setVoteOrSurveyId(String voteOrSurveyId) {
		this.voteOrSurveyId = voteOrSurveyId;
	}
	public int getSortNum() {
		return sortNum;
	}
	public void setSortNum(int sortNum) {
		this.sortNum = sortNum;
	}
	
}
