package com.longtime.ajy.model;

import com.longtime.common.model.Entity;


public class Template implements Entity<String>{


	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 1L;

	private String id; //模版id
	
	private String name;//模版名称

	private String remark;//说明
	
	private String mid;//模块ID
	
	private String view_path;//jsp页面相对路径
	
	private String cover; //图片地址
	
	private boolean is_default=false;//是否是默认模版
	
	private int type=0;//0 所有人可用 1 部分人可用
	
	private int isdel=0;//标记删除 0可用 1已删除
	

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getMid() {
		return mid;
	}

	public void setMid(String mid) {
		this.mid = mid;
	}

	public String getView_path() {
		return view_path;
	}

	public void setView_path(String view_path) {
		this.view_path = view_path;
	}

	public String getCover() {
		return cover;
	}

	public void setCover(String cover) {
		this.cover = cover;
	}

	public boolean isIs_default() {
		return is_default;
	}

	public void setIs_default(boolean is_default) {
		this.is_default = is_default;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public int getIsdel() {
		return isdel;
	}

	public void setIsdel(int isdel) {
		this.isdel = isdel;
	}
	
}
