*{padding:0px;margin:0px;}
ul{list-style:none;}
.clr{clear:both;}
a,a:hover,a:focus{text-decoration:none;}
input,textarea,a{outline:none;}
textarea{resize:none;}
input:focus,textarea:focus,a:focus{outline:none;}
:root{height:100%;}
body{font-family:"Microsoft yahei","sans-serif";font-size:62.5%;background:#eeeeee;color:#53575e;min-width:320px;height:100%;}
body a,button,input{-webkit-tap-highlight-color:rgba(255,0,0,0);}
article.scroll a:active{background:#f4f4f4;}
div.gradeA ul.a a:active,div.gradeB ul.b a:active{background:#f4f4f4;}

header{width:100%;height:38px;line-height:38px;padding:6px 0px;text-align:center;font-size:16px;color:#fff;position:fixed;z-index:5;top:0px;left:0px;}
.home{width:44px;height:38px;text-align:center;line-height:38px;position:absolute;right:0px;top:9px;}
.home i{font-size:24px;width:44px;height:38px;line-height:38px;display:block;}
.home a{color:#fff;}
.home a.txt{font-size:12px;}
.back{width:44px;height:38px;line-height:38px;position:absolute;left:0px;top:9px;}
.back i{font-size:24px;}
.back a{color:#fff;width:44px;height:38px;line-height:38px;display:block;}
header span.addWrap{height:38px;width:70%;white-space:nowrap;text-overflow:ellipsis;display:inline-block;line-height:38px;overflow:hidden;}

article.scroll{padding-top:50px;z-index:0;}

footer.footer{display:none;}

section.info{width:100%;background:#fff;}
section.info a{width:auto;padding:15px 15px 20px 15px;height:65px;box-shadow:0px 0px 3px #ccc;}
section.info ul{float:left;width:260px;}
section.info ul li{float:left;margin-left:15px;width:180px;}
section.info ul li.pic{width:65px;margin-left:0px;}
section.info ul li.pic img{width:65px;height:65px;}
section.info ul li.name {color: #53575e;font-size: 16px;height: 16px;line-height: 16px;margin-bottom: 14px;}


/*新版本个人信息模块*/
section ul li.cont1{font-size:16px;height:16px;line-height:16px;color:#999;}


nav{width:auto;padding:5px;overflow:hidden;}

nav .menu{width:auto;height:20px;line-height:20px;padding:15px 15px 15px 18px;margin-bottom:5px;background:#fff;box-shadow:0px 0px 3px #ccc;}
nav .menu i{width:20px;height:20px;font-size:20px;float:left;}
nav .menu span{margin-left:20px;height:20px;line-height:20px;font-size:16px;float:left;}
nav .menu a{height:20px;font-size:16px;float:right;color:#aaa;}
nav .menu a i{width:16px;height:20px;text-align:center;}


/*主页的第二种模式 模块*/
section.info a{display:block;color:#53575e;overflow:hidden;}
section.info span.arrow{display:block;width:26px;height:26px;line-height:26px;font-size:20px;text-align:center;float:right;color:#aaa;margin:19px 0px 20px 0px;}

nav a.menu{display:block;color:#53575e;}
nav .menu span.a{height:20px;font-size:16px;float:right;color:#aaa;margin-left:0px;}
nav .menu span.a i{width:16px;height:20px;text-align:center;}

section.msg{padding:20px 10px 38px 10px;width:auto;overflow:hidden;}
section.msg div{width:auto;overflow:hidden;background:#fff;border-radius:5px;box-shadow:0px 0px 5px #ccc;color:#999;font-size:16px;padding:8px;}
section.msg div textarea{border:none;min-height:152px;color:#999;width:auto;}
section.msg p.textarea+p{height:16px;width:auto;line-height:16px;padding:8px;text-align:right;}

a.sub{display:block;margin:0 10px;width:auto;height:38px;font-size:16px;color:#fff;line-height:38px;text-align:center;}
a.sub2{margin-top:28px;}

section.password{padding:15px 10px 0px;width:auto;overflow:hidden;}
section.password div{padding:11px;width:auto;height:16px;border-radius:5px;box-shadow:0px 0px 5px #ccc;background:#fff;margin-top:22px;}
section.password input{width:100%;height:16px;line-height:16px;border:none;font-size:16px;color:#999;}
section.password div:first-child{margin-top:0px;}

section.join{padding:8px 5px;width:auto;overflow:hidden;}
section.join a{width:auto;display:block;overflow:hidden;height:38px;padding:10px 8px;background:#fff;margin-top:5px;box-shadow:0px 0px 3px #ccc;}
section.join a:first-child{margin-top:0px;}
section.join a ul{width:100%;}
section.join a li{width:100%;font-size:12px;color:#999;height:12px;line-height:12px;overflow:hidden;}
section.join a li:first-child{width:100%;font-size:16px;color:#53575e;height:16px;line-height:16px;margin-bottom:10px;}

section.test{padding:8px 5px;width:auto;overflow:hidden;}
section.test ul{padding:10px 8px 8px 8px;height:40px;width:auto;overflow:hidden;background:#fff;margin-top:5px;box-shadow:0px 0px 3px #ccc;}
section.test ul:first-child{margin-top:0px;}
section.test ul li{display:block;height:20px;width:100%;font-size:12px;line-height:20px;float:left;color:#999;}
section.test ul li span{display:block;width:50px;height:20px;line-height:20px;float:left;font-size:12px;color:#fff;text-align:center;border-radius:10px;margin-right:20px;}
section.test ul li span.b{background:#ccc;}
section.test ul li:first-child{height:16px;line-height:16px;font-size:16px;margin-bottom:4px;color:#53575e;}
section.test ul li label{margin-right:20px;color:#999;}


section.result{padding:20px 0px 40px;width:100%;overflow:hidden;}
section.result ul{width:200px;margin:0 auto;text-align:center;}
section.result ul li:first-child{height:16px;line-height:16px;font-size:16px;width:100%;}
section.result ul li.face_g{font-size:120px;height:120px;line-height:120px;margin:18px 0px 40px 0px;}
section.result ul li.txt{margin-top:12px;font-size:16px;height:16px;line-height:16px;color:#999;}
section.result ul li.txt span{color:#53575e;padding-left:5px;}
section.result ul li.txt1{margin-top:0px;}

/*该模块可删除*/
section.onlineTest{padding:8px 5px;width:auto;overflow:hidden;}
section.onlineTest a{width:auto;display:block;overflow:hidden;height:38px;padding:10px 8px;background:#fff;margin-top:5px;box-shadow:0px 0px 3px #ccc;}
section.onlineTest a:first-child{margin-top:0px;}
section.onlineTest a ul{width:100%;}
section.onlineTest a li{width:100%;font-size:13px;color:#999;height:13px;line-height:13px;}
section.onlineTest a li:first-child{width:100%;font-size:16px;color:#53575e;height:16px;line-height:16px;margin-bottom:10px;}

section.option{padding:5px;width:auto;overflow:hidden;background:#fff;box-shadow:0px 0px 3px #ccc;}
section.option a{display:block;padding:4px 9px;border:1px #fff solid;text-align:center;height:16px;line-height:16px;font-size:16px;color:#999;width:auto;float:left;}

section.testOnline {padding:5px;width:auto;overflow:hidden;}
section.testOnline a{width:auto;display:block;overflow:hidden;height:68px;padding:10px 8px;background:#fff;margin-top:5px;box-shadow:0px 0px 3px #ccc;}
section.testOnline a:first-child{margin-top:0px;}
section.testOnline a ul{width:100%;overflow:hidden;}
section.testOnline a li{width:100%;font-size:12px;color:#999;height:12px;line-height:12px;margin-top:8px;float:left;}
section.testOnline a li:first-child{width:100%;font-size:16px;color:#53575e;height:16px;line-height:16px;margin-top:0px;margin-bottom:4px;}
section.testOnline a li.state{height:20px;line-height:20px;font-size:12px;}
section.testOnline a li.state label{color:#fff;background:#ccc;width:60px;height:20px;text-align:center;float:right;font-size:12px;border-radius:10px;}

section.more{padding:5px 55px 25px 55px;width:auto;overflow:hidden;}
section.more a{width:100%;height:36px;line-height:36px;text-align:center;border:1px solid #999;color:#999;display:block;font-size:14px;}

section.loading{padding:25px 55px;width:auto;overflow:hidden;text-align:center;height:36px;line-height:36px;font-size:28px;color:#999;}


section.aLoading{width:50px;overflow:hidden;text-align:center;height:36px;line-height:36px;font-size:28px;color:#999;position:absolute;top:125px;left:50%;margin-left:-25px;}




section.list{height:36px;line-height:36px;width:100%;overflow:hidden;background:#fff;box-shadow:0px 0px 3px #ccc;}
section.list a{margin:9px 14px;width:16px;height:18px;line-height:18px;display:block;font-size:18px;float:right;text-align:center;}

section.news{padding:5px;width:auto;overflow:hidden;}
section.news a{width:100%;overflow:hidden;margin-top:5px;background:#fff;display:block;box-shadow:0px 0px 3px #ccc;}
section.news a:first-child{margin-top:0px;}
section.news div{float:left;width:68px;height:60px;padding:8px 0px 10px 8px;}
section.news div img{width:68px;height:39px;}
section.news ul{float:left;margin-left:10px;padding:8px 8px 10px 0px;width:68%;}
section.news ul li{width:100%;float:left;}
section.news ul li.ttl{color:#53575e;font-size:16px;min-height:48px;}
section.news ul li.date{color:#999;height:12px;line-height:12px;}


section.learnOnline{padding:8px 5px;width:auto;overflow:hidden;}
section.learnOnline a{ width:100%;overflow:hidden;background:#fff;overflow:hidden;display:block;box-shadow:0px 0px 3px #ccc;height:58px;margin-top:5px;}
section.learnOnline a:first-child{margin-top:0px;}
section.learnOnline div{float:left;width:68px;height:40px;padding:10px 0px 10px 8px;}
section.learnOnline div img{width:68px;height:39px;}
section.learnOnline div.prompt{width:50px;float:right;padding:10px 8px 10px 0px;font-size:12px;line-height:60px;color:#999;text-align:right;}
section.learnOnline div.prompt i{font-size:14px;margin-left:2px;}
section.learnOnline ul{float:left;margin-left:10px;padding:10px 0px;}
section.learnOnline ul li{width:100%;float:left;overflow:hidden;}
section.learnOnline ul li.ttl{color:#53575e;font-size:16px;line-height:16px;height:16px;margin-bottom:4px;text-overflow:ellipsis;white-space:nowrap;}
section.learnOnline ul li.date{color:#999;height:18px;line-height:24px;font-size:12px;}
section.learnOnline ul li.date span{float:right;height:18px;line-height:12px;font-size:12px;}
section.learnOnline ul li.date span i{font-size:18px;margin-left:8px;height:18px;}


/*弹出层*/
div.popBox{z-index:10;position:fixed;left:0px;top:0px;width:100%;height:100%;}
div.showPopup{opacity:.5;-moz-opacity:.5;filter:alpha(opacity=50);position:fixed;left:0px;right:0px;top:0px;bottom:0px;background:#000;z-index:20;}
div.gradeA{height:100%;position:fixed;right:0px;top:0px;background:#fff;width:230px;z-index:30;overflow:hidden;}
div.gradeA .header{height:38px;padding:6px 0px;postion:relative;background:#eee;width:100%;text-align:center;font-size:16px;line-height:38px;}
div.gradeA .header .back{width:38px;height:38px;line-height:38px;position:absolute;left:0px;top:6px;}
div.gradeA .header .back i{font-size:18px;}
div.gradeA .header .back a{color:#999;width:38px;height:38px;line-height:38px;display:block;}
div.gradeA ul.a{padding:0px 5px;width:220px;overflow-y:scroll;}
div.gradeA ul.a li{width:220px;height:36px;border-bottom:1px solid #eee;overflow:hidden;}
div.gradeA ul.a li a.a{display:block;padding:0px 20px 0px 20px;background:url(../img/arr_r.png) 207px center no-repeat;height:36px;font-size:14px;color:#53575e;line-height:36px;color: #53575e;overflow:hidden;}
div.gradeA ul.a li a.b{display:block;padding:0px 20px 0px 20px;height:36px;font-size:14px;color:#53575e;line-height:36px;color: #53575e;overflow:hidden;}
div.gradeA ul.a li a i{font-size:18px;float:right;color:#999;}
div.gradeA ul.a li a.a:active{background:#f4f4f4 url(../img/arr_r.png) 207px center no-repeat;}
div.gradeA ul.a li a.b:active{background:#f4f4f4;}


section.comment{margin:8px 5px;padding:8px 2px;background:#fff;width:auto;overflow:hidden;box-shadow:0px 0px 3px #ccc;}
section.comment span.count{display:block;width:auto;padding:0px 6px;margin-bottom:2px;height:16px;font-size:16px;}
section.comment span.count label{margin-left:5px;font-size:12px; }
section.comment ul{width:100%;border-top:1px solid #eee;overflow:hidden;padding:14px 0px;position:relative;}
section.comment span.count+ul{border-top:none;}
section.comment ul li{float:left;display:block;}
section.comment ul li.pic{width:50px;padding-left:6px;}
section.comment ul li.pic img{width:50px;height:50px;}
section.comment ul li.per{margin-left:10px;padding-right:6px;color:#999;font-size:16px;line-height:16px;width:60%;}
section.comment ul li.per span{position:absolute;right:6px;top:14px;font-size:12px;}
section.comment ul li.per+li{margin-left:10px;padding-right:6px;width:60%;margin-top:10px;font-size:14px;height:14px;line-height:14px;color:#999;}
section.comment ul li.say{clear:left;margin-top:8px;font-size:16px;height:16px;line-height:16px;padding:0 6px;}

section.sender{padding:5px;background:#e0e0e0;position:fixed;z-index:5;left:0px;bottom:0px;height:36px;min-width:310px;width:auto;float:left;overflow:hidden;}
section.sender input{padding:8px;line-height:20px;font-size:14px;color:#999;background:#fff;border:none;width:75%;border-radius:3px;}
section.sender a{display:block;float:right;width:55px;height:36px;line-height:36px;font-size:16px;color:#fff;text-align:center;margin-left:5px;border-radius:3px;box-shadow:0px 0px 3px #ccc;}


section.complete{padding:8px 5px;width:auto;overflow:hidden;}
section.complete a{ width:100%;overflow:hidden;background:#fff;overflow:hidden;display:block;box-shadow:0px 0px 3px #ccc;height:76px;margin-top:5px;}
section.complete a:first-child{margin-top:0px;}
section.complete div{float:left;width:68px;height:60px;padding:8px 0px 8px 8px;}
section.complete div img{width:68px;height:39px;}
section.complete div.prompt{width:50px;float:right;padding:8px 8px 8px 0px;font-size:12px;line-height:102px;color:#999;text-align:right;}
section.complete div.prompt i{font-size:14px;margin-left:2px;}
section.complete ul{float:left;margin-left:8px;padding:8px 0px;width:53%;}
section.complete ul li{width:100%;float:left;overflow:hidden;}
section.complete ul li.ttl{color:#53575e;font-size:16px;line-height:16px;height:16px;}
section.complete ul li.date{color:#999;height:12px;line-height:12px;font-size:12px;margin-top:10px;}
section.complete ul li.date span{float:right;height:18px;line-height:12px;font-size:12px;}
section.complete ul li.date span i{font-size:18px;margin-left:8px;height:18px;}

section.infoMore{width:auto;padding:8px 5px;overflow:hidden;}
section.infoMore ul{width:100%;background:#fff;box-shadow:0px 0px 3px #ccc;margin-top:5px;}
section.infoMore ul:first-child{margin-top:0px;}
section.infoMore ul li{padding:7px 15px 7px 20px;display:block;border-top:1px solid #eee;height:36px;line-height:36px;font-size:16px;}
section.infoMore ul li:first-child{border-top:none;}
section.infoMore ul li span{float:right;height:36px;line-height:36px;font-size:14px;display:block;}
section.infoMore ul li span img{width:36px;height:36px;}


section.infoEdit{width:auto;padding:8px 5px;overflow:hidden;}
section.infoEdit ul{width:100%;background:#fff;box-shadow:0px 0px 3px #ccc;margin-top:5px;}
section.infoEdit ul:first-child{margin-top:0px;}
section.infoEdit ul li{padding:7px 15px 7px 20px;display:block;border-top:1px solid #eee;height:36px;line-height:36px;font-size:16px;}
section.infoEdit ul li:first-child{border-top:none;}
section.infoEdit ul li span{float:right;height:36px;line-height:36px;font-size:14px;display:block;}
section.infoEdit ul li span img{width:36px;height:36px;}
section.infoEdit ul li span img:hover{cursor:pointer;}
section.infoEdit ul li span input{height:18px;line-height:18px;padding:6px 8px;width:180px;border:none;border:1px solid #eee;border-radius:3px;color:#53575e;font-size:14px;z-index:0;}
section.infoEdit ul li.gender span{color:#53575e;width:190px;text-align:left;line-height:36px;}
section.infoEdit ul li.gender input{width:15px;margin:0px 5px 0px 20px;vertical-align:middle;}
section.infoEdit ul li.gender span input:first-child{margin-left:0px;}

.popBg{color:#000;opacity:.5;-moz-opacity:.5;filter:alpha(opacity=50);position:absolute;left:0px;right:0px;top:50px;bottom:0px;background:#000;z-index:0;height:100%;}
.picBox{position:fixed;bottom:15px;left:5px;right:5px;height:144px;overflow:hidden;}
.picBox ul{width:100%;margin-top:15px;overflow:hidden;background:#fff;border-radius:3px;}
.picBox ul:first-child{margin-top:0px;}
.picBox ul li{border-top:1px solid #eee;height:42px;line-height:42px;width:100%;}
.picBox ul li a{display:block;width:100%;text-align:center;height:42px;line-height:42px;font-size:16px;}
.picBox ul li:first-child{border-top:none;}

section.login{width:auto;padding:70px 32px 32px 32px;overflow:hidden;}
section.login div{padding:6px 12px;height:24px;line-height:24px;border-radius:3px;width:auto;margin-top:22px;background:#fff;}
section.login div:first-child{margin-top:0px;}
section.login input{width:100%;font-size:20px;border:none;height:24px;line-height:24px;color:#999;}
section.login a{font-size:18px;width:100%;height:38px;line-height:38px;color:#fff;display:block;text-align:center;margin-top:36px;}

p.error{padding-left:5px;height:12px;margin-top:10px;font-size:12px;line-height:12px;}


section.showNews{width:auto;padding:12px 5px 15px 5px;overflow:hidden;}
section.showNews h2{font-size:16px;font-weight:normal;color:#53575e;padding-left:5px;margin-bottom:10px;}
section.showNews span.date{padding-left:5px;display:block;font-size:12px;color:#999;margin-bottom:5px;}
section.showNews p{font-size:16px;line-height:24px;padding:5px 0px;}
section.showNews p.pic{padding:5px;}
section.showNews p.pic img{width:100%;}

p.error{display:none;}

section.noData{width:100%;padding:30px 0 125px 0;overflow:hidden;text-align:center;height:115px;line-height:115px;}
section.noData img{height:115px;}


span.tishi {width:80px;height:14px;padding:5px 20px;line-height:14px;color:#fff;background:#333;font-size:14px;display:block;position:fixed;left:50%;margin-left:-60px;bottom:50px;text-align:center;}
span.tishi1 {width:80px;height:14px;padding:5px 20px;line-height:14px;color:#fff;background:#333;font-size:14px;display:block;position:fixed;left:50%;margin-left:-60px;bottom:80px;text-align:center;}

div.placeHolder{height:20px;width:100%;}


section.activity{padding:8px 5px 0px 5px;width:auto;overflow:hidden;}
section.activity a{display:block;margin-top:5px;background:#fff;box-shadow:0px 0px 3px #ccc;}
section.activity a:first-child{margin-top:0px;}
section.activity ul{width:100%;overflow:hidden;padding-bottom:10px;}
section.activity a ul li{padding:0px 8px;display:block;color:#53575e;overflow:hidden;}
section.activity a ul li:first-child{padding:0px;margin-bottom:8px;width:100%;}
section.activity ul li:first-child img{width:100%;display:block;}
section.activity ul li:first-child+li{margin-bottom:10px;font-size:16px;height:20px;line-height:20px;}
section.activity ul li:first-child+li+li{font-size:13px;color:#999;text-align:right;height:13px;line-height:13px;}

div.radio{height:16px;padding:20px 10px 0px 10px;line-height:16px;}
div.radio span{height:16px;margin-right:20px;width:38px;padding-left:30px;display:block;float:left;font-size:16px;color:#999;background:url(../img/de.png) 3px center no-repeat;}
div.radio .checked{background:url(../img/ch.png) 3px center no-repeat;}

article.index{width:100%;position:relative;background:url(../img/i_bg.jpg) left bottom no-repeat;background-size:cover;}
article.index ul{position:absolute;top:32px;right:32px;width:196px;}
article.index ul li{width:62px;margin-bottom:22px;text-align:center;font-size:14px;font-weight:bold;float:left;margin-left:36px;}
article.index ul li a{color:#fff;display:block;}
article.index ul li span{height:16px;line-height:16px;}
article.index ul img{width:62px;height:62px;margin-bottom:10px;}
article.index span.logo img{height:28px;position:absolute;bottom:24px;left:26px;display:block;}

span.wrap{width:180px;height:38px;line-height:38px;overflow:hidden;display:inline-block;text-overflow:ellipsis;white-space:nowrap; }

/*默认首页*/
article.default{width:100%;height:100%;overflow:scroll;/*display:-moz-box;display:-webkit-box;display:box;-moz-box-orient:vertical;-webkit-box-orient:vertical;box-orient:vertical;*/}
article.default section.banner{width:100%;height:auto;}
article.default section.banner img{width:100%;height:100%;display:block;}
article.default nav{/*-moz-box-flex:2;-webkit-box-flex:2;box-flex:2;*/background:#fff;padding:0px;margin:0px;}
article.default nav div{width:100%;height:33%;display:block;}
article.default nav div a{display:inline-block;width:33%;height:100%;box-sizing:border-box;float:left;border-right:1px solid #f1f1f1;border-bottom:1px solid #f1f1f1;text-align:center;color:#53575e}
article.default nav div ul{height:100%;width:100%;vertical-align:middle;display:table;margin:0 auto;}
article.default nav div ul li{text-align:center;display:table-cell;padding:0px;width:100%;height:100%;vertical-align:middle;}
article.default nav div ul li img{width:62px;box-sizing:border-box;}
article.default nav div ul li span{display:block;font-size:12px;}
article.default nav div a:last-child{border-right:none;width:34%;}
article.default nav div:last-child{height:34%;}
article.default nav div:last-child a{border-bottom:none;}	
/*主题颜色*/	
article.default nav div a:active{background:#f4f4f4;color:#009933;}

article.default nav span.img{position:relative;display: inline-block;}
article.default nav span.img label{background-color: #e55769;border:2px solid #fff;width:24px;height: 24px;border-radius: 24px;z-index: 100;position:absolute;right:-16px;top:-12px;line-height:24px;text-align: center;font-size:12px;color:#fff;}


