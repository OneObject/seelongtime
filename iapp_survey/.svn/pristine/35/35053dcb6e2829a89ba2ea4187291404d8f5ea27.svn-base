package com.myctu.platform.gateway.agent;

import com.myctu.platform.protocol.exception.ProcessException;

/**
 * @since 2011-12-1
 * @author pippo
 */
public class GatewayAgentException extends ProcessException implements GatewayAgentErrorCode {

	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 7734787364356666134L;

	public GatewayAgentException(int code, String message, Throwable cause) {
		super(code, message, cause);
	}

	public GatewayAgentException(int code, String message) {
		super(code, message);
	}

	public GatewayAgentException(int code, Throwable cause) {
		super(code, cause);
	}

	public GatewayAgentException(String message, Throwable cause) {
		super(message, cause);
	}

	public GatewayAgentException(String message) {
		super(message);
	}

	public GatewayAgentException(Throwable cause) {
		super(cause);
	}

}
