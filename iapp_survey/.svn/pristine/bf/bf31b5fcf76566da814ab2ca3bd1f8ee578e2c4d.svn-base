.align_right {
	text-align: right;
}

.img {
	width: 22px;
	height: 22px;
}

.img_con {
	width: 50px;
}

.content_td {
	width: 100%;
	height: 80px;
}

.content_td div {
	width: 99% !important;
	height: 98% !important;
	border: none;
}

.content_area {
	width: 99%;
	height: 90%;
}

.quickenter {
	display: block;
	text-align: center;
	color: #fff;
	cursor: pointer;
}

.enter-big {
	width: 100%;
	height: 130px;
}

.enter-big h1 {
	line-height: 130px;
}

.blue {
	background-color: #27A9E3;
}

.green {
	background-color: #28B779;
}

.orange {
	background-color: #FFB848;
}
.quickenterlist{
padding: 10px;
display: block;overflow: hidden;
}
.quickenterlist ul,.quickenterlist ul li{
	display:block;
	margin: 0px;
	padding: 0px;
}
.quickenterlist ul li{
	float: left;
	margin-right: 20px;
	color:#fff;
	font-size: 20px;
	font-weight: bold;
	margin-bottom: 20px;
}
.enter-small{
	width: 100px;
	height: 100px;
	text-align: center;
	display:block;
	line-height: 100px;
}
