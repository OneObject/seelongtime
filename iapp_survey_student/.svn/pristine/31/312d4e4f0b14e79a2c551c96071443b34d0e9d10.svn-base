/*课程分类——————————————————————————————————————————————————————*/
html,body{
    font-family: "microsoft yahei";
}
ul li{
    list-style: none!important;
}
/* Swipe 2 required styles */
.swipe {
    overflow: hidden;
    visibility: hidden;
    position: relative;
}
.swipe-wrap {
    overflow: hidden;
    position: relative;
}
.swipe-wrap > div {
    float:left;
    width:100%;
    position: relative;
}
.swipe-wrap > div >img{
    width:100%;
}
nav #position{
    position: absolute;
    z-index: 10;
    margin-top: -20px;
    margin-left: 30%;;
}
#position li{
    float: left;
    width: 10px;
    height: 10px;
    background-color: #ffffff;
    /*border: 1px inset #ffffff;*/
    border-radius: 50%;
    margin-left: 5px;
    list-style: none;
}
#position .on{background-color: #D9534F}
/* END required styles */
.panel-default > .panel-heading{
    background-image: none;
}
.panel-heading{
    padding: 0px;
}
.panel-title>a:hover,.panel-title>a:focus{
    text-decoration: none;
    /*background-color: #D9534F;*/
    /*color: #ffffff;*/
}
.panel-title>a{
    padding: 10px 15px;
    font-size: 20px;
    font-weight: bold;
    display: block;
    width: 100%;
    color: #3D4450;
}
.panel-title>a>i:first-child{
    float: left;
    font-size: 20px;
    color: #3D4450;
    margin-right: 5px;
    margin-top: 2px;
}
.panel-title>a>i:last-child{
    float: right;
    margin-right: 5px;
}
.class-title{
    font-weight: 600;
}
.class-list,.class-list li{
    margin: 0px;
    padding: 0px;
    display: block;
    overflow: hidden;
    width: 100%;
}
.class-list li{
    padding: 10px 20px;
    font-size: 14px;
    border-bottom: 1px solid #dadada;
    cursor: pointer;
}
.class-list li:hover,.class-list li:focus{
    background-color: #fafafa;
}
.class-list li>i{
    float: right;
    color: #888888;
}
/*====================================================================================================================*/
/*课程列表*/
.course-list,.course-list li{
    margin: 0px;
    padding: 0px;
    display: block;
    overflow: hidden;
    width: 100%;
}
.course-list li{
    border:1px solid #dadada;
    border-radius: 5px;
    padding: 10px 10px;
    margin-top: 10px;
}
.course-list-item{
    width: 100%;
    height: 80px;
    display: block;
    overflow: hidden;
}
.course-list-item-img{
    width: 80px;
    height:80px;
    display: block;
    overflow: hidden;
}
.course-list-item-img img{
    width: 80px;
    height: 80px;
    border-radius: 5px;
}
.course-list-item-img .i{
    font-size: 92px;
    position: absolute;
    z-index: 2;
    margin-top: -80px;
    margin-left: 0px;
    color: #999;
    opacity: 0.8;-moz-opacity: 0.8;filter:alpha(opacity=80);
}
.course-list-item-detail{
    width: 100%;
    height: 80px;
    display: block;
    padding-left: 90px;
    margin-top: -85px;
}
.course-list-item-detail h3{
    margin: 0px;
    padding-left: 0px;
    font-size: 1em;
    line-height: 1em;
    width: 100%;
    display: block;
    overflow: hidden;
    height: 24px;
    line-height: 24px;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.course-list-item-detail span{
    font-size: 75%;
    height: 42px;
    line-height: 21px;
    display: block;overflow: hidden;
}
.course-list-item-detail p{
    color: #999;
    font-size: 75%;
    display: block;
    height: 24px;
    line-height: 24px;
    text-align: right;
    /*margin-top: 12px;*/
}
.course-list-item-detail p i:first-child{
    margin-left: 0px;
}
.course-list-item-detail p i{
    margin-left: 20px;
}
/*课程系列*/
.series{
    display: block;
    overflow: hidden;
    padding: 0px 10px;
}
.a-series{
    border: 1px solid #dadada;
    border-radius: 5px;
    padding: 10px 0px;
    margin-bottom: 20px;
    margin-top: 10px;
}
.series-top{
    display: block;
    overflow: hidden;
    padding: 0px 10px;
}
.series-top img{
    width: 100%;
    z-index: 1;
    zoom: 1;
}
.series-top-title-bg{
    position: absolute;
    background-color: #999999;
    display: block;
    overflow: hidden;
    text-align: center;
    width:85%;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    margin-top: -30px;
    z-index: 2;
    opacity: 0.7;-moz-opacity: 0.7;filter:alpha(opacity=70);
}
.series-top-title{
    position: absolute;
    display: block;
    overflow: hidden;
    text-align: center;
    width:85%;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    margin-top: -30px;
    z-index: 3;
}
.a-series .course-list li{
    border: none;
    padding: 10px;
    border-bottom: 1px solid #dadada;
}
.a-series .course-list li:last-child{
    border: none;
}
.course-name{
    width: 100%;
    font-size: 1em;
    max-height: 42px;
    line-height: 20px;
    display: block;
    overflow: hidden;
    margin-bottom: 10px;
}
.class-time{
    font-size: 75%;
    color: #999999;
}
.jianjie{
    width: 100%;
    padding: 5px 10px;
    display: block;
    font-size: 75%;
    overflow: hidden;
}
.quanwen{
    width: 100%;
    height: 24px;
    line-height: 24px;
    font-size: 1em;
    padding: 0px 10px;
}
.quanwen p{
    color: #999;
    font-size: 75%;
    display: block;
    height: 24px;
    line-height: 24px;
}
