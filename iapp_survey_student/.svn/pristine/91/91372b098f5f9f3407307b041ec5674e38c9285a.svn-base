body{
	font-family: "microsoft yahei", "sans-serif";
	font-size:14px;
    background-color:#f8f6f1;
    min-width:320px;
}
section,div,ul,dl,ol,a,span,h1,h2,h3,h4,li{
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
a{
	color:#444;
	text-decoration: none;
}
a:visited{
	color:#444;
}
section.banner{
	position:relative;
}
section.search{
    width:100%;
    height:48px;
    line-height:48px;
    background-color:#529bea;
    padding:8px 12px 8px 48px;
    margin-bottom: 8px;
    position:relative;
}
section.search .back{
	display:inline-block;
	width:48px;
	height:48px;
	position:absolute;
	left:0;
	top:0;
	z-index:50;
	background:url(../img/back.png) center center no-repeat;
	background-size:auto 24px;
}
.search-box{
    width:100%;
    height:32px;
    line-height: 32px;
    position:relative;
}
.search-box input{
    display: block;
    padding: 4px 34px 4px 10px;
    width:100%;
    height:100%;
    color:#999;
    line-height: 24px;
    float:left;
    border: 0;
    border-radius: 16px;
    font-size:14px;
    box-shadow: 1px 1px 2px rgba(0,0,0,0.3) inset,-1px -1px 2px rgba(0,0,0,0.3) inset;
}
.search-box span{
    position: absolute;
    right: 6px;
    height:32px;
    width:32px;
    top:0;
    background:url(../img/search.png) center center no-repeat;
    background-size: 24px 24px;
    cursor:pointer;
}
section.nav{
    width:100%;
    margin-bottom:8px;
}
nav{
    width:100%;
    overflow: hidden;
    background: #529bea;
}
nav a{
    display: block;
    width:33%;
    height:90px;
    float:left;
    border-top:1px solid #fff;
}
nav a:nth-child(1),nav a:nth-child(2),nav a:nth-child(3){
    border-top:0;
}
nav a:nth-child(3n+2){
    width:34%;
    border-left:1px solid #fff;
    border-right:1px solid #fff;
}
nav a img{
    display: block;
    height:84px;
    margin:3px auto;
}
section.product{
    width:100%;
}
.product-show{
    width:100%;
    margin-bottom: 8px;
    overflow: hidden;
}
.product-show a{
    display: block;
    color:#333;
    font-size:14px;
    background: #fff;
    min-height: 100px;
    float:left;
    position: relative;
    text-decoration: none;
}

.show-cols-2 a:first-child{
    width:67%;
}
.show-cols-2 a:last-child{
    width:33%;
    background: #f9f9f9;
}
.show-cols-2 a{
    padding:8px 10px;
    height: 214px;
}
.show-cols-2 a h2{
    font-size:18px;
    font-weight: bold;
    color:#1e85cf;
}
.show-cols-2 a h3{
    font-size:15px;
    font-weight: bold;
    color:#ff8700;
}
.show-cols-2 a:first-child dl{
    width:210px;
    margin:0 auto;
    margin-top:10px;
}
.show-cols-2 a:first-child dt{
    font-size:18px;
    margin-bottom:10px;
    color:#529bea;
}
.show-cols-2 a:first-child dd{
    position:relative;
}
.show-cols-2 a:first-child dd span{
    display:block;
    width:110px;
    font-size:13px;
    line-height: 17px;
    color:#666;
    position: absolute;
    right:0px;
    top:10px;
}
.show-cols-2 a:first-child dd img{
    display: block;
    float:left;
    height:115px;
}
.show-cols-2 a:last-child dl{
    width:110px;
    margin:0 auto;
    margin-top:10px;
}
.show-cols-2 a:last-child dt{
    color:#666;
    line-height: 20px;
}
.show-cols-2 a:last-child img{
    width:100px;
    margin: 0 auto;
}
.show-cols-2 .more{
    position: absolute;
    right:10px;
    bottom:8px;
    font-size:12px;
    color:#529bea;
}
.show-cols-3 a{
    width:33%;
    background: #f9f9f9;
    padding:8px 10px;
    overflow: hidden;
}
.show-cols-3 a:nth-child(2){
    width:34%;
    background: #fff;
}
.show-cols-3 h4{
    font-size:14px;
    color:#333;
    margin-bottom: 5px;
}
.show-cols-3 .index-img-box img{
    display: block;
    height: 85px;
    margin:0 auto;
}
.show-cols-3 ul li img{
    display: block;
    max-width: 100px;
    max-height: 85px;
}
label.new,label.hot{
    display: block;
    width:32px;
    height:32px;
    position: absolute;
    top:0;
    right:0;
}
label.new{
    background: url(../img/new_icon.png) center center no-repeat;
    background-size: 100% 100%;
}
label.hot{
    background: url(../img/hot_icon.png) center center no-repeat;
    background-size: 100% 100%;
}
.swiper-wrapper img{
    display: block;
    width:100%;
}
.pagination {
	position: absolute;
	left: 0;
	text-align: center;
	bottom: 5px;
	width: 100%;
	z-index: 100;
}
.swiper-pagination-switch {
	display: inline-block;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	width: 12px;
	height: 12px;
	border-radius: 12px;
	background: #444;
	border:2px solid #888;
	margin: 0 4px;
	cursor: pointer;
}
.swiper-active-switch {
	background: #fff;
}
.no-margin{
    margin:0 !important;
}
.category-list{
    width:100%;
    padding:0 8px;
    background: #fff;
}
.list-with-img{
    width:100%;
}
.list-with-img li{
    width:100%;
    display: block;
    border-bottom: 1px solid #eee;
    height:55px;
    line-height: 54px;
    padding-right:40px;
    background: url(../img/arr1.png) right center no-repeat;
    background-size:auto 20px;
}
.list-with-img li:last-child{
    
}
.list-with-img li img{
    display: block;
    width:40px;
    height:40px;
    margin:6px 10px;
    float:left;
}
.list-with-img li span{
    display: block;
    height:54px;
    line-height: 54px;
    font-size:18px;
    padding:0 5px 0 90px;
}

.list-no-img{
    width:100%;
}
.list-no-img li{
  width: 100%;
  display: block;
  border-bottom: 1px solid #eee;
  min-height: 55px;
  line-height: 54px;
  padding-right: 40px;
  background: url(../img/arr1.png) right center no-repeat;
  background-size: auto 20px;
}
.list-no-img li:last-child{
   
}
.list-no-img li span{
	  display: block;
	  min-height: 54px;
	  line-height: 24px;
	  font-size: 18px;
	  padding: 15px 5px 15px 12px;
}
.height-99px li{
    height:99px!important;
    line-height: 98px!important;
}
.height-99px span{
    height:98px!important;
    line-height: 98px!important;
}
.product-list{
    width:100%;
    background: #fff;

}
.product-list a{
    display: block;
    width:100%;
    padding:10px 12px 10px 12px;
    position: relative;
    color:#444;
    text-decoration: none;
    border-bottom: 1px solid #eee;
    overflow:hidden;
}
.product-list a > label{
    position:absolute;
    right:0;
    top:0;
    width:24px;
    height:24px;
}
.product-list label.new{
    background: url(../img/new1.png) center center no-repeat;
    background-size: 24px 24px;
}
.product-list label.hot{
    background: url(../img/hot1.png) center center no-repeat;
    background-size: 24px 24px;
}
.product-list .info-box{
    width:100%;
}
.info-box img{
    display: block;
    width:120px;
    float:left;
}
.info-box ul{
    width:100%;
    padding-left:132px;
}
.info-box ul li:first-child{
    font-size:16px;
}
.info-box ul li:nth-child(2){
    font-size: 14px;
    margin-top:6px;
    color:#666;
}
.info-box ul li:nth-child(3){
    font-size: 14px;
    margin-top:6px;
    color:#666;
}
.info-box ul li:nth-child(4){
    font-size: 12px;
    margin-top:6px;
    color:#666;
    height:16px;
    line-height: 16px;
}
.info-box li:nth-child(4){
    text-align: right;
}
.info-box .zan,.info-box .ping{
    margin:0 6px 0 12px;
    display: inline-block;
    width:16px;
    height:16px;
    float:right;
}
.info-box li:nth-child(4) span{
    display: inline-block;
    height:16px;
    float:right;
}
.info-box .zan{
    background: url(../img/zan.png) center center no-repeat;
    background-size: 16px 16px;
}
.info-box .ping{
    background: url(../img/ping.png) center center no-repeat;
    background-size: 16px 16px;
}
.bg-white{
    background: #fff!important;
}
.product-intro{
    width:100%;
    padding:5px 10px;
}
.product-intro li{
    color:#444;
    font-size:16px;
    height: 28px;
    line-height: 28px;
}
.img-box{
    width: 100%;
    padding:0 8px;
}
.img-box img{
    display: block;
    width:100%;
}
.tab{
    width:100%;
    overflow: hidden;
    background: #fff;
    padding:0 8px;
    margin-bottom: 5px;
}
.tab-line-4{
	width: 100%;
	height:36px;
	line-height: 34px;
	overflow: hidden;
}
.tab-line-4 span:first-child{
    border:1px solid #529bea;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}
.tab-line-4 span:last-child{
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}
.tab-line-4 span{
	display: block;
	float:left;
	width:25%;
	text-align: center;
	border: 1px solid #539bea;
    border-left:0;
	color: #539bea;
	background-color: #fff;
	font-size:16px;
}
.tab-line-4 span.active{
	color: #fff;
	background-color: #529bea;
}
.tab-line-3{
	width: 100%;
	height:36px;
	line-height: 36px;
	border:1px solid #529bea;
	border-radius:5px;
	overflow: hidden;
}
.tab-line-3 span{
	display: block;
	float:left;
	width:33%;
	text-align: center;
	border-right: 1px solid #539bea;
	color: #539bea;
	background-color: #fff;
	font-size:16px;
}
.tab-line-3 span:nth-child(2){
	width:34%;
}
.tab-line-3 span.active{
	color: #fff;
	background-color: #529bea;
}
.tab-line-3 span:last-child{
	border:none;
}
.error-404{
	width:100%;
	height:240px;
	position: absolute;
	left:0px;
	top:50%;
	margin-top:-140px;
}
.error-404 img{
	display: block;
	width:125px;
	margin:0 auto;
}
.error-404 a{
	display: block;
	margin:0 auto;
	border:1px solid #529bea;
	border-radius: 5px;
	color:#529bea;
	width:90px;
	height:34px;
	line-height: 34px;
	text-align: center;
	margin-top:40px;
}

table{
    width: 100%;
    border-collapse: collapse;
    border-bottom: 1px solid #eee;
}
table tr{
    width: 100%;
}
table tbody tr:last-child{
}
table tbody tr td{
    text-align: left;
    vertical-align: middle;
    border: none;
    padding:5px;

}
table tbody td img{
    display: block;
    width: 100%;
}

table.line td{
    width: 100%;
    padding:10px 5px;
}
table.line-1-1 td{
    width: 50%;
}

table.line-1-2  tr td:first-child{
    width: 33%;
}
table.line-1-2  tr td:last-child{
    width: 67%;
}
table.line-2-1  tr td:first-child{
    width: 67%;
}
table.line-2-1  tr td:last-child{
    width: 33%;
}

table.line-3-2  tr td:first-child{
    width: 60%;
}
table.line-3-2  tr td:last-child{
    width: 40%;
}
table.line-2-3  tr td:first-child{
    width: 40%;
}
table.line-2-3  tr td:last-child{
    width: 60%;
}

table.line-3-1  tr td:first-child{
    width: 75%;
}
table.line-3-1  tr td:last-child{
    width: 25%;
}
table.line-1-3  tr td:first-child{
    width: 25%;
}
table.line-1-3  tr td:last-child{
    width: 75%;
}

.wrapper-box{
    padding:8px;
}

div.line-title {
    height: 30px;
    padding: 0 10px;
    color: #fff;
    font-size: 16px;
    line-height: 30px;
    margin: 10px 0;
    background-image: -webkit-gradient(linear, top left, top right, color-stop(0%, #529bea), color-stop(100%, #fff));
    background-image: -webkit-linear-gradient(left, #529bea 0%, #fff 100%);
    background-image: -moz-linear-gradient(left, #529bea 0%, #fff 100%);
    background-image: -ms-linear-gradient(left, #529bea 0%, #fff 100%);
    background-image: -o-linear-gradient(left, #529bea 0%, #fff 100%);
    background-image: linear-gradient(to right, #529bea 0%, #fff 100%);
}


.version{
    padding:10px 5px;
    border-bottom: 1px solid #eee;
}
.version table{
    width: 100%;
    border-collapse: collapse;
    border-bottom: 0;
}
.version tr{
    width: 100%;
}
.version tbody tr td{
    text-align: left;
    vertical-align: middle;
    border: 1px solid #444;
    padding:5px;
}
.version tbody tr td:nth-child(1){
    width: 20%;
}
.version tbody tr td:nth-child(2){
    width: 20%;
}
.version tbody tr td:nth-child(3){
    width: 60%;
}
.holder-no-data{
	width:100%;
	height:40px;
	line-height:40px;
	text-align:center;
	font-size:24px;
	color:#aaa;
	position:absolute;
	top:40%;
	margin-top:-20px;
}
.holder-no-data1{
	width:100%;
	height:40px;
	line-height:40px;
	text-align:center;
	font-size:20px;
	color:#aaa;
	position:absolute;
	top:76%;
	margin-top:-20px;
}
.holder-no-data2{
	position:static;
	margin-top:40px;
}

.holder-loading{
    width: 100%;
    text-align: center;
    height: 40px;
    line-height: 40px;
    color:#999;
    font-size:16px;
	margin:10px 0;
}
.holder-loading i{
    font-size:20px;
    margin-right:5px;
}
.bg-white{
	background:#fff;
}
.dialog-wrapper{
    position:fixed;
    left:0;
    top:0;
    right:0;
    bottom:0;
    background-color:rgba(0,0,0,0.5);
    z-index:100;
}
.dialog-box{
    border-radius:4px;
    border:1px solid #ccc;
    position:fixed;
    left:50%;
    top:35%;
    background:#fff;
    z-index:105;
    text-align:center;
    margin-left:-150px;
}
.dialog-box .dialog-header{
    min-height:44px;
    line-height:44px;
    font-size:16px;
    color:#529bea;
    border-bottom:2px solid #529bea;
    padding:0 15px;
    text-align: left;
}
.dialog-box .dialog-content{
    font-size:16px;
    color:#53575e;
    border-bottom:1px solid #ccc;
    padding:15px;
    line-height:22px;
    text-align: left;
}
.dialog-box .dialog-footer{
    min-height:44px;
    line-height:44px;
    font-size:16px;
    color:#53575e;
}
.dialog-box .dialog-footer span{
    text-align:center;
    display:inline-block;
    cursor:pointer;
    width:50%;
    color:#529bea;
}
.dialog-box .dialog-footer span:last-child{
    box-sizing:border-box;
    border-left:1px solid #ccc;
}
.dialog-box .dialog-footer.cols-1 span{
    width:100%;
}
