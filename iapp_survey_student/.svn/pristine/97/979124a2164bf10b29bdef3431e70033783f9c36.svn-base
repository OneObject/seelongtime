package com.longtime.ajy.support;

public class AppMobileException extends AppException{

	/**
	 * 
	 */
	private static final long serialVersionUID = 7799321508477318503L;
	
	public AppMobileException(){
        super();
    }
	// msgcode 对应 message-info.properties 中的key
    public AppMobileException(String msgcode){
        super(msgcode);
    }

    public AppMobileException(String msgcode, Throwable cause){
        super(msgcode, cause);
    }

    public AppMobileException(Throwable cause){
        super(cause);
    }

}
