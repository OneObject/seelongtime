body {
  font-family: "FontAwesome", "Microsoft YaHei"  !important; 
  color:#f32d51;
  background:#f1f1f1;}





section .main-box{
  width:100%;
}
section .main-box a{
  display:table;
  font-size:15px;
  width:33%;
  float:left;
  color:#f32d51;
  border-right:1px solid #deb4c7;
  border-bottom:1px solid #deb4c7;
}
section .main-box a.a-3{
  width:34%;
  border-right:none;
}
section .main-box a div{
  display:table-cell;
  vertical-align:middle;
  width:100%;
  text-align:center;
}
section .main-box a span{
  display:block;
  width:50px;
  height:50px;
  border-radius:25px;
  background:#f67d7d;
  margin:0 auto;
}
section .main-box a label{
  display:block;
  margin-top:5px;
}
section .main-box a span.main-color-2{
  background:#fec383;
}
section .main-box a span.main-color-3{
  background:#fecf7e;
}
section .main-box a span.main-color-4{
  background:#44c186;
}
section .main-box a span.main-color-5{
  background:#8a76ee;
}
section .main-box a span.main-color-6{
  background:#13ACEB;
}
section .main-box a span.main-color-7{
  background:#fe7c2e;
}
section .main-box a span.main-color-8{
  background:#8e8e8e;
}
section .main-box a span.main-color-9{
  background:#44d8e1;
}
section .main-box a img{
  width:25px;
  margin-top:30%;
}


#footer-box{
  width:100%;
  height:auto;
  background:#f1f1f1;
}
footer{
  width:90%;
  height:auto;
  margin:0 auto;
}
footer .footer-title{
  width:100%;
  height:60px;
  line-height:60px;
  color:#f6a6c7;
  font-size:20px;
  margin:0;
}
footer .footer-nav{
  font-size:12px;
}
footer .footer-nav p{
  position:absolute;
  bottom:5px;
  width:100%;
  text-align:center;
  color:#f6a6c7;
}
footer .footer-nav a{
  display:inline-block;
  float:left;
  width:23%;
  height:auto;
  border:2px solid #f6a6c7;
  margin-right:2%;
  color:#f6a6c7;
  padding-top:3%;
  padding-bottom:3%;
}
footer .footer-nav a div{
  width:100%;
  text-align:center;
  margin-top:6%;
}
footer .footer-nav a span{
  display:block;
  margin:0 auto;
}
footer .footer-nav a img{
  width:25px;
}
footer .footer-nav a.footer-subnav{
  margin-right:0;
}

