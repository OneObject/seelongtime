	<title>成长路</title>
	<link rel="stylesheet" href="${basepath }/static/m_h5/ceyuan/css/tree.css" type="text/css">
	<script type="text/javascript" src="${basepath }/static/m_h5/js/jquery-2.1.1.min.js"></script>
		
	<script type="text/javascript">
		$(function (){
			$('.bg').show();


			var bg = $('.bg').width();                   // 父元素
			var conWid = $('.container').width();        // 顶部六边形宽度
			var sentWid = (bg - conWid) / 2;

			var autoWid = $('.container-auto').width();  //居中子六边宽度
			var sentautoWid = (bg - autoWid) / 2 + 10;

			var rightWid = $('.container-right').width(); //左右子边宽度
			var posleftWid = sentautoWid + autoWid -18;


			var xSend = (bg - sentautoWid) - rightWid*2 + 35;

			$('.container-0').css('left',sentWid);
			$('.container-auto').css('left',sentautoWid);
			$('.container-right').css('left',posleftWid);
			$('.container-left').css('left',xSend);
		})
	</script>
</head>
<body>
	<div style="display:none;" class="bg">
		<div class="container container-0 ">
			<a href="http://www.aijiayou.com/student/m/site/index/ff8080814f92dd20014fade573183ac3.xhtml?fdomain=resource">
				<img src="${basepath }/static/m_h5/ceyuan/img/0_0.png" alt="">
			</a>
		</div>
		<div class="container container-right container-1">
