.bg{
  position:relative;
}
.bg img{
  width:100%;
}
.content{
  width:100%;
  position:absolute;
  left:0;
  top:0px;
}
.content .logo a{
  display:block;
  width:100%;
  height:130px;
  line-height:205px;
  text-align:center;
}
.content .logo img{
  width:80%;
  height:auto;
  margin:0;
}
.content .search_box{
  width:100%;
  text-align:center;
  margin-top:2px;
}
.content .search_box input{
  width:80%;
  height:50px;
  border-radius:4px;
  border:1px solid #f73569;
  padding:0 10px;
  font-size:18px;
}
.content .search_box input.search_text{
  margin-bottom:20px;
}
.content .search_box input::-webkit-input-placeholder{
  color:#ccc;
}

.content .search_box p{
  color:#ff6da4;
  font-size:16px;
  margin:10px 0;
}
.content .search_box .search_sub{
  width:40%;
  position:relative;
}
.content .search_box .search_sub img{
  width:40%;
  margin:0;
}
.content .search_box .search_sub i{
  position :absolute;
  left:25%;
  top:-150%;
  font-style:normal;
  color:#fff;
  font-size:23px;
  letter-spacing:20px;
}
@media only screen and (min-width:414px){
  .content .search_box .search_sub i{
    top:-180%;
    left:30%;
  }
}
@media only screen and (min-width:375px){
  .content .search_box .search_sub i{
    top:-170%;
    letter-spacing: 28px;
  }
}