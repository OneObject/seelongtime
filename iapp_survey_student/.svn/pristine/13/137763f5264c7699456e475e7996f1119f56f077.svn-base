package com.longtime.ajy.model;

import com.longtime.common.model.BaseEntity;


public class Collection extends BaseEntity<Long> {

	private Long id;
	private Integer restype = RESTYPE_QUESTION;
	private String uid;
	private Long timestamp;
	private String resid;
    
	public static final int RESTYPE_QUESTION=1; //问题
	






	public Integer getRestype() {
		return restype;
	}

	public void setRestype(Integer restype) {
		this.restype = restype;
	}

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public Long getTimestamp() {
		return timestamp;
	}

	public void setTimestamp(Long timestamp) {
		this.timestamp = timestamp;
	}

	public String getResid() {
		return resid;
	}

	public void setResid(String resid) {
		this.resid = resid;
	}

	public void setId(Long id) {
		this.id = id;
	}

 

    @Override
    public Long getId() {
        return id;
    }
}
