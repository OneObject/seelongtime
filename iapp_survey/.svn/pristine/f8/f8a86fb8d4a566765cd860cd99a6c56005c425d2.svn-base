package com.longtime.app.ixin.mgr.vote.vo;

import java.util.List;

public class QuestionVO {
	
	private String qid;//投票问题的ID
	private String qtitle;//问题
	private List<OptionVO> options; //选项
	private int voteNum;
	private int shortNum;
	
	public int getShortNum() {
		return shortNum;
	}
	public void setShortNum(int shortNum) {
		this.shortNum = shortNum;
	}
	public int getVoteNum() {
		return voteNum;
	}
	public void setVoteNum(int voteNum) {
		this.voteNum = voteNum;
	}
	public String getQid() {
		return qid;
	}
	public void setQid(String qid) {
		this.qid = qid;
	}
	public String getQtitle() {
		return qtitle;
	}
	public void setQtitle(String qtitle) {
		this.qtitle = qtitle;
	}
	public List<OptionVO> getOptions() {
		return options;
	}
	public void setOptions(List<OptionVO> options) {
		this.options = options;
	}
	
}
