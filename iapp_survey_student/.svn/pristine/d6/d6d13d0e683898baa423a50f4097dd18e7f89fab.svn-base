article.survey{width:100%;height:100%;background:#f8f8f8;}
.survey section.infoBox{padding:15px 10px 12px;box-sizing:border-box;width:100%;background:#fff;border-bottom:1px solid #e0e0e0;}
.survey section.infoBox ul{width:100%;}
.survey section.infoBox ul li{width:100%;font-size:12px;line-height:12px;color:#888;}
.survey section.infoBox ul li span{float:right;display:inline-block;font-size:14px;margin-top:-16px;height:16px;line-height:16px;}
.survey section.infoBox ul li span i{margin-right:4px;}
.survey section.infoBox ul li:first-child{height:auto;min-height:16px;font-size:16px;line-height:18px;margin-bottom:15px;color:#53575e;}
.survey section.infoBox ul li:last-child{margin-top:12px;line-height:14px;}
.survey section.infoBox p.msg{border:1px solid;width:100%;text-align:center;font-size:16px;height:52px;line-height:50px;box-sizing:border-box;margin-bottom:20px;}

.survey section.questionBox{padding:14px 8px;box-sizing:border-box;width:100%;overflow:hidden;}
.survey section.questionBox .qHead{height:16px;line-height:16px;padding-bottom:10px;border-bottom:1px solid #e0e0e0;}
.survey section.questionBox .qHead span:first-child{font-size:16px;color:#53575e;}
.survey section.questionBox .qHead span:last-child{font-size:12px;color:#888;float:right;margin-right:10px;}
.survey section.questionBox dl{padding:18px 0px 20px;width:100%;}
.survey section.questionBox dl dt{margin-bottom:15px;font-size:16px;line-height:16px;}
.survey section.questionBox dl dt label{font-size:12px;color:#888;}
.survey section.questionBox dl dd{-moz-display:box;-webkit-display:box;display:box;overflow:hidden;margin-top:12px;border:1px solid #f2f2f2;padding:10px;min-height:36px;background:#fff;line-height:18px;box-sizing:border-box;font-size:16px;}
.survey section.questionBox dl dt+dd{margin-top:0px;}
.survey section.questionBox dl dd span.select{margin-right:10px;border:1px solid #e4e4e4;height:16px;width:16px;line-height:16px;display:inline-block;border-radius:16px;}
.survey section.questionBox dl dd span.select2{border-radius:0px;}
.survey section.questionBox dl dd span.select input{-moz-opacity:0;filter:alpha(opacity=0);opacity:0;}
.survey section.questionBox dl dd span:last-child{-moz-box-flex:1;-webkit-box-flex:1;box-flex:1;line-height:18px;}
.survey section.questionBox dl dd span.dan{background:url(../img/dian.png) center center no-repeat;background-size:100% 100%;}
.survey section.questionBox dl dd span.duo{background:url(../img/duo.png) center center no-repeat;background-size:100% 100%;}
.survey section.questionBox dl dd textarea{border:none;width:100%;font-size:16px;overflow:hidden;min-height:44px;word-wrap:break-word;color:#53575e;}
.survey section.questionBox dl dd p{padding-bottom:8px;width:100%;}
.survey section.questionBox dl dd p img{width:100%;display:block;}

.survey section.submit{padding:20px 8px 34px 8px;box-sizing:border-box;width:100%;}
.survey section.submit span{display:block;border:1px solid;font-size:16px;height:36px;line-height:36px;text-align:center;cursor:pointer;}

.survey section.questionBox dl.grade{padding-bottom:0px;}
.survey section.questionBox dl.grade dd{background:none;border:none;padding:0px;display:box;}
.survey section.questionBox dl.grade dd label{float:left;display:inline-block;font-size:16px;text-align:center;margin:0 15px 15px 0;border:1px solid #E0E0E0;background:#fff;color:#707070;cursor:pointer;}
.survey section.questionBox dl.grade dd label.cur{color:#fff;}

.survey section.questionBox dl.result dd{-moz-display:box;-webkit-display:box;display:box;float:left;width:100%;line-height:16px;box-sizing:border-box;}
.survey section.questionBox dl.result dd span{line-height:16px;}
.survey section.questionBox dl.result dd span:first-child{width:84px;float:left;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.survey section.questionBox dl.result dd span:last-child{width:46px;float:right;text-align:right;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.survey section.questionBox dl.result dd span.per{-moz-box-flex:1;-webkit-box-flex:1;box-flex:1;height:16px;line-height:16px;display:inline-block;}
.survey section.questionBox dl.result dd span.per label{height:12px;background:#627f26;border-radius:4px;display:inline-block;}
.survey section.questionBox dl dt.label{position:relative;}
.survey section.questionBox dl dt.label label.block{position:absolute;width:2px;height:14px;display:inline-block;background-color:#627f26;left:0px;top:1px;}
.survey section.questionBox dl dt.label span{display:inline-block;margin-left:5px;}

.survey section.msgBox{padding:34px 8px;box-sizing:border-box;width:100%;}
.survey section.msgBox p{border:1px solid;width:100%;text-align:center;font-size:16px;height:52px;line-height:50px;box-sizing:border-box;}
.survey section.msgBox2{padding:42px 8px;}
.survey section.msgBox2+p{font-size:16px;color:#53575e;width:100%;height:16px;line-height:16px;padding:0px;margin:0px;text-align:center;}

.survey dl.sel_dan dd,.survey dl.sel_duo dd{cursor:pointer;}

.survey section.resultBox{padding:12px 8px;box-sizing:border-box;width:100%;overflow:hidden;}
.survey section.resultBox .rHead{height:16px;line-height:16px;padding:0px 0px 14px;font-size:16px;}
.survey section.resultBox dl{padding-bottom:14px;}
.survey section.resultBox dl dt{padding-bottom:10px;font-size:14px;line-height:14px;}
.survey section.resultBox dl dd{margin-bottom:14px;box-sizing:border-box;padding:10px;border:1px solid #f2f2f2;background:#fff;min-height:36px;overflow:hidden;font-size:14px;}
.survey section.resultBox dl dd p{padding-bottom:10px;width:100%;}
.survey section.resultBox dl dd p img{width:100%;display:block;}
.survey section.resultBox dl dd span{display:inline-block;font-size:14px;height:14px;line-height:14px;float:left;}
.survey section.resultBox dl dd p+span,.survey section.resultBox dl dd span:first-child{width:50px;margin-right:5px;text-align:center;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.survey section.resultBox dl dd span.per+span{width:62px;padding:0px 4px;color:#627f26;float:right;}
.survey section.resultBox dl dd span.per+span i{margin-right:2px;}
.survey section.resultBox dl dd span.per{height:14px;line-height:14px;}
.survey section.resultBox dl dd span.per label.b{display:inline-block;width:38px;color:#999;font-size:14px;text-align:center;margin-left:5px;}
.survey section.resultBox dl dd span.per label.a{height:14px;background:#627f26;border-radius:4px;display:inline-block;}
.survey section.resultBox dl dd textarea{border:none;width:100%;font-size:16px;overflow:hidden;height:auto;word-wrap:break-word;color:#53575e;background:#fff;font-size:14px;}
.survey section.resultBox dl dd label.color{color:#627f26;margin-left:5px;font-size:14px;}
.survey section.resultBox dl dt.label{position:relative;}
.survey section.resultBox dl dt.label label.block{position:absolute;width:2px;height:14px;display:inline-block;background-color:#627f26;left:0px;top:1px;}
.survey section.resultBox dl dt.label span{display:inline-block;margin-left:5px;}

.survey section.resultBox dl.result1 dd{-moz-display:box;-webkit-display:box;display:box;float:left;width:100%;line-height:16px;box-sizing:border-box;}
.survey section.resultBox dl.result1 dd span{line-height:16px;}
.survey section.resultBox dl.result1 dd span:first-child{width:84px;float:left;overflow:hidden;margin-right:0px;white-space:nowrap;text-overflow:ellipsis;text-align:left;height:16px;}
.survey section.resultBox dl.result1 dd span:last-child{width:46px;float:right;text-align:right;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;color:#53575e;padding:0px;}
.survey section.resultBox dl.result1 dd span.per{-moz-box-flex:1;-webkit-box-flex:1;box-flex:1;height:16px;line-height:16px;display:inline-block;}
.survey section.resultBox dl.result1 dd span.per label{height:12px;background:#627f26;border-radius:4px;display:inline-block;}


/*考试模块*/
article.kaoshi{width:100%;height:100%;background:#f8f8f8;}
.kaoshi section.infoBox{padding:13px 14px;height:12px;display:block;border-bottom:1px solid #e0e0e0;text-align:center;color:#999;}
.kaoshi section.infoBox span{font-size:12px;color:#999;}
.kaoshi section.infoBox span:first-child{float:left;}
.kaoshi section.infoBox span:last-child{float:right;}

.kaoshi section.questionBox{padding:14px 8px 54px 8px;box-sizing:border-box;width:100%;overflow:hidden;}
.kaoshi section.questionBox dl:first-child{padding-top:0px;}
.kaoshi section.questionBox dl{padding:18px 0px 20px;width:100%;}
.kaoshi section.questionBox dl dt{margin-bottom:15px;font-size:16px;line-height:16px;}
.kaoshi section.questionBox dl dt label.num{font-size:16px;color:#53575e;}
.kaoshi section.questionBox dl dt label{font-size:12px;color:#888;}
.kaoshi section.questionBox dl dt span.i{display:inline-block;height:16px;line-height:16px;width:22px;margin-left:10px;text-align:center;}
.kaoshi section.questionBox dl dt span.i i{font-size:18px;color:#000;}
.kaoshi section.questionBox dl dd{-moz-display:box;-webkit-display:box;display:box;overflow:hidden;margin-top:12px;border:1px solid #f2f2f2;padding:10px;min-height:36px;background:#fff;line-height:18px;box-sizing:border-box;font-size:16px;}
.kaoshi section.questionBox dl dt+dd{margin-top:0px;}
.kaoshi section.questionBox dl dd span.select{margin-right:10px;border:1px solid #e4e4e4;height:16px;width:16px;line-height:16px;display:inline-block;border-radius:16px;}
.kaoshi section.questionBox dl dd span.select2{border-radius:0px;}
.kaoshi section.questionBox dl dd span.select input{-moz-opacity:0;filter:alpha(opacity=0);opacity:0;}
.kaoshi section.questionBox dl dd span:last-child{-moz-box-flex:1;-webkit-box-flex:1;box-flex:1;line-height:18px;}
.kaoshi section.questionBox dl dd span.dan{background:url(../img/dian.png) center center no-repeat;background-size:100% 100%;}
.kaoshi section.questionBox dl dd span.duo{background:url(../img/duo.png) center center no-repeat;background-size:100% 100%;}
.kaoshi section.questionBox dl dd textarea{border:none;width:100%;font-size:16px;overflow:hidden;min-height:44px;word-wrap:break-word;color:#53575e;}
.kaoshi section.questionBox dl dd p{padding-bottom:8px;width:100%;}
.kaoshi section.questionBox dl dd p img{width:100%;display:block;}
.kaoshi dl.sel_dan dd,.kaoshi dl.sel_duo dd{cursor:pointer;}

.kaoshi section.questionBox dl dd span.dan{background-color:#627f26;}
.kaoshi section.questionBox dl dd span.duo{background-color:#627f26;}

.kaoshi section.questionBox div.errorMsg{height:36px;background:#f2dede;color:#d64156;font-size:16px;line-height:36px;text-align:center;margin:15px 12px 25px 12px;}
.kaoshi section.questionBox dl dd.correct{border-color:#bbd782;}

.kaoshi section.questionBox ul.score{padding:16px 0px 90px;text-align:center;}
.kaoshi section.questionBox ul.score1{padding-bottom:65px;}
.kaoshi section.questionBox ul.score li:first-child{font-size:16px;color:#53575e;padding-bottom:28px;} 
.kaoshi section.questionBox ul.score li:first-child+li{font-size:76px;color:#627f26;padding-bottom:64px;height:72px;line-height:72px;}
.kaoshi section.questionBox ul.score2 li:first-child+li{font-size:14px;color:#627f26;padding-bottom:0px;height:14px;line-height:14px;}
.kaoshi section.questionBox ul.score li:last-child{color:#999;font-size:14px;}
/*正确错误，颜色设置*/
.kaoshi section.questionBox ul.score li.a{font-size:16px;color:#bbd782;height:16px;line-height:16px;margin-bottom:26px;margin-top:-10px;}
.kaoshi section.questionBox ul.score li.b{font-size:16px;color:#d64156;height:16px;line-height:16px;}

.kaoshi section.questionBox ul.score li label{width:28px;display:inline-block;text-align:center;}
.kaoshi section.questionBox div.retry{border:1px solid;font-size:16px;height:36px;line-height:36px;text-align:center;cursor:pointer;border-color:#627f26;color:#627f26;}

.kaoshi section.infoBox1{font-size:16px;height:16px;line-height:16px;}
.kaoshi section.questionBox div.retry1{font-size:16px;height:38px;line-height:38px;text-align:center;}
.kaoshi section.questionBox div.retry1 span{display:inline-block;border:1px solid;border-color:#627f26;color:#627f26;height:36px;line-height:36px;width:130px;cursor:pointer;}
.kaoshi section.questionBox div.retry1 span:first-child{margin-right:10px;}
.kaoshi section.questionBox div.retry1 span:first-child i{display:inline-block;height:16px;font-size:16px;margin-right:5px;position:relative;top:1px;}



.kaoshi section.questionBox .rank{height:14px;padding-top:8px;line-height:14px;font-size:14px;color:#627f26;width:80px;}
.kaoshi section.questionBox .rank i{font-size:14px;display:inline-block;width:20px;height:14px;}


.kaoshi section.guideBox{height:44px;line-height:44px;display:block;width:100%;background:#fff;border-top:1px solid #e0e0e0;position:fixed;left:0px;bottom:0px;}
.kaoshi section.guideBox span{display:inline-block;height:44px;line-height:44px;font-size:14px;color:#627f26;padding:0px 10px;cursor:pointer;}
.kaoshi section.guideBox span:active{background:#f4f4f4;}
.kaoshi section.guideBox span i{font-size:16px;height:16px;display:inline-block;}
.kaoshi section.guideBox span:first-child{float:left;}
.kaoshi section.guideBox span:first-child i{margin-right:10px;}
.kaoshi section.guideBox span:first-child+span{width:140px;text-align:center;position:absolute;top:0px;left:50%;margin-left:-60px;padding:0px;}
.kaoshi section.guideBox span:last-child{float:right;}
.kaoshi section.guideBox span:last-child i{margin-left:10px;}

/*通用提示框模块*/
div.dialogBg{z-index:1000;position:fixed;left:0px;top:0px;width:100%;height:100%;overflow:hidden;background-color:#000;-moz-opacity:.5;filter:alpha(opacity=50);opacity:.5;}
div.dialogBox{width:280px;border-radius:4px;border:1px solid #ccc;position:fixed;left:50%;top:50%;margin-top:-81px;margin-left:-141px;background:#fff;z-index:10000;}
div.dialogBox .dialogHeader{min-height:44px;line-height:44px;font-size:16px;color:#627f26;border-bottom:1px solid #627f26;padding:0px 15px;}
div.dialogBox .dialogContent{font-size:16px;color:#53575e;border-bottom:1px solid #ccc;padding:16px 15px;}
div.dialogBox .dialogContent span{line-height:22px;}
div.dialogBox .dialogFooter{min-height:44px;line-height:44px;font-size:16px;color:#53575e;}
div.dialogBox .dialogFooter span{text-align:center;display:inline-block;cursor:pointer;}
div.dialogBox .dialogFooter span:active{background:#f4f4f4;}
div.dialogBox .dialogFooter span.a{width:50%;}
div.dialogBox .dialogFooter span.b{width:50%;box-sizing:border-box;border-left:1px solid #ccc;}
div.dialogBox .dialogFooter span.c{width:100%;}

/*考试详情页*/
article.kaoshiDetail{width:100%;height:100%;padding:0px 8px;box-sizing:border-box;background:#f8f8f8;}
.kaoshiDetail section.enterKaoshi{padding:16px 0px 38px;}
.kaoshiDetail section.enterKaoshi ul li:first-child{color:#53575e;font-size:16px;margin-bottom:10px;line-height:16px;}
.kaoshiDetail section.enterKaoshi ul li:first-child+li{color:#999;font-size:12px;height:12px;line-height:12px;margin-bottom:10px;}
.kaoshiDetail section.enterKaoshi ul li.pic img{width:100%;display:block;}
.kaoshiDetail section.enterKaoshi div.start{margin-top:20px;font-size:16px;border:1px solid #627f26;color:#627f26;text-align:center;height:38px;box-sizing:border-box;line-height:36px;}
.kaoshiDetail section.moreInfo{padding-bottom:20px;}
.kaoshiDetail section.moreInfo ul li{margin-bottom:12px;color:#999;font-size:12px;height:12px;line-height:12px;}
.kaoshiDetail section.moreInfo .rank{height:14px;padding-top:8px;line-height:14px;font-size:14px;color:#627f26;width:80px;}
.kaoshiDetail section.moreInfo .rank i{font-size:14px;display:inline-block;width:20px;height:14px;}

/*问答模块*/
article.wenda{width:100%;height:100%;padding:0px 8px;box-sizing:border-box;background:#f8f8f8;}
article.wenda .wendaBox{padding-bottom:40px;}
article.wenda .wendaBox a{display:block;margin-top:8px;padding:8px;background:#fff;box-shadow:0px 0px 3px #ccc;color:#53575e;}
article.wenda .wendaBox a ul{width:100%;overflow:hidden;}
article.wenda .wendaBox a ul li{float:left;width:70%;}
article.wenda .wendaBox a ul li:first-child{width:48px;height:48px;margin-right:10px;}
article.wenda .wendaBox a ul li:first-child img{width:48px;height:48px;display:block;}
article.wenda .wendaBox a ul li:first-child+li{font-size:16px;height:16px;line-height:16px;margin-top:6px;}
article.wenda .wendaBox a ul li:last-child{height:12px;line-height:12px;color:#999;font-size:12px;margin-top:10px;}
article.wenda .wendaBox a p{font-size:14px;line-height:22px;padding:6px 0px 8px 0px;}
article.wenda .wendaBox a>div{overflow:hidden;color:#999;line-height:14px;}
article.wenda .wendaBox a>div span:first-child{float:left;}
article.wenda .wendaBox a>div span:first-child label{display:inline-block;margin:0px 4px;color:#627f26;}
article.wenda .wendaBox a>div span:last-child{float:right;}
article.wenda .wendaBox a>div span:last-child i{display:inline-block;margin:0px 4px;font-size:13px;height:13px;}
article.wenda .optionBox{width:100%;height:48px;line-height:48px;border-top:1px solid #e0e0e0;position:fixed;left:0px;bottom:0px;background:#fff;z-index:10000;}
article.wenda .optionBox span:first-child{width:100px;height:48px;position:absolute;left:50%;top:0px;margin-left:-50px;text-align:center;font-size:16px;color:#627f26;cursor:pointer;}
article.wenda .optionBox span.options{width:48px;height:48px;position:absolute;right:0px;top:0px;text-align:center;font-size:24px;color:#627f26;cursor:pointer;border-left:1px solid #e0e0e0;}
article.wenda .optionBox ul{position:absolute;bottom:49px;right:1px;background:#fff;border:1px solid #e0e0e0;}
article.wenda .optionBox ul li{line-height:36px;height:36px;padding:0 22px;font-size:16px;color:#999;border-top:1px solid #e0e0e0;margin:0px 2px;cursor:pointer;text-align:center;}
article.wenda .optionBox ul li:first-child{border:none;}

article.wenda1{padding:0px;}
article.wenda .wenti{padding:4px 10px 10px;background:#fff;border-bottom:1px solid #e0e0e0;}
article.wenda .wenti p{font-size:14px;line-height:22px;padding-bottom:10px;}
article.wenda .wenti ul li:first-child{margin-bottom:10px;font-size:12px;color:#999;}
article.wenda .wenti ul li:first-child label{display:inline-block;margin:0px 4px;}
article.wenda .wenti ul li:last-child{overflow:hidden;color:#999;}
article.wenda .wenti ul li:last-child span:first-child{float:left;}
article.wenda .wenti ul li:last-child span:last-child{float:right;}
article.wenda .wenti ul li:last-child span:last-child i{display:inline-block;margin:0px 4px;font-size:13px;height:13px;}
article.wenda .huida{padding:8px;box-sizing:border-box;width:100%;margin-bottom:30px;}
article.wenda .huida div.box{padding:8px;background:#fff;overflow:hidden;margin-bottom:8px;box-shadow:0px 0px 3px #ccc;}
article.wenda .huida div.box>div{width:48px;float:left;}
article.wenda .huida div.box>div img{width:48px;height:48px;display:block;}
article.wenda .huida div.box ul{float:left;display:block;padding-left:10px;}
article.wenda .huida div.box ul li:first-child{font-size:16px;height:16px;line-height:16px;padding:6px 0px 10px;}
article.wenda .huida div.box ul li:first-child+li{font-size:14px;line-height:22px;padding-bottom:8px;}
article.wenda .huida div.box ul li:last-child{font-size:12px;color:#999;}

article.wenda .huida span.noAnswer{display:block;width:120px;position:relative;left:50%;margin-left:-60px;margin-top:80px;}
article.wenda .huida span.noAnswer img{width:120px;display:block;}

article.wenda .txt{padding:10px;}
article.wenda .txt div{padding:8px;box-sizing:border-box;width:100%;background:#fff;border-radius: 5px;box-shadow: 0px 0px 5px #CCC;margin-bottom:12px;}
article.wenda .txt div textarea{width:100%;height:130px;font-size:16px;line-height:22px;border:none;color:#999;}
article.wenda .txt div+span{display:inline-block;float:right;height:30px;line-height:30px;font-size:16px;color:#fff;background:#627f26;padding:0px 15px;border-radius: 3px;cursor:pointer;}

.ctl_header{display:none;}
.ctl_footer{display:none;}
article.ctl_cont{padding-top:50px;}


/*新版课程详情评论模块*/
.funcBox{width:100%;height:48px;line-height:48px;border-top:1px solid #e0e0e0;position:fixed;left:0px;bottom:0px;background:#fff;z-index:12000;}
.funcBox span{display:inline-block;height:48px;font-size:14px;color:#627f26;box-sizing:border-box;width:32%;text-align:center;cursor:pointer;}
.funcBox span:first-child+span{border-left:1px solid #e0e0e0;border-right:1px solid #e0e0e0;box-sizing:border-box;}
.funcBox span:first-child i{display:inline-block;margin-right:5px;position:relative;top:0px;}

.funcBox1{width:100%;height:48px;line-height:48px;border-top:1px solid #e0e0e0;position:fixed;left:0px;bottom:0px;background:#fff;z-index:12000;}
.funcBox1 span{display:inline-block;height:48px;font-size:14px;color:#627f26;box-sizing:border-box;width:49%;text-align:center;cursor:pointer;}
.funcBox1 span:first-child+span{border-left:1px solid #e0e0e0;}
.funcBox1 span:first-child i{display:inline-block;margin-right:5px;position:relative;top:0px;}
 
/*作业模块*/ 
article.zuoye{width:100%;height:100%;background:#f8f8f8;}
article.zuoye nav{padding:8px 30px;box-sizing:border-box;width:100%;background:#fff;border-bottom:1px solid #e0e0e0;margin-bottom:8px;}
article.zuoye nav ul{width:100%;border:1px solid #627f26;border-radius:5px;float:left;}
article.zuoye nav ul li{height:26px;line-height:26px;width:50%;display:inline-block;text-align:center;box-sizing:border-box;float:left;font-size:16px;cursor:pointer;color:#627f26;}
article.zuoye nav ul li:first-child+li{border-left:1px solid #627f26;}
