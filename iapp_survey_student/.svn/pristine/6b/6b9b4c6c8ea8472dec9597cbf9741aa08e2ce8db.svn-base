package com.longtime.ajy.model;

import com.longtime.ajy.mweb.model.TestQuestionModel;



public class TestQuestionVo extends TestQuestionModel {
	

	private static final long serialVersionUID = -8261663224112051097L;

	private int weight; //排序，值越大越靠前
	
	private String user_answer; //答案
	
	private String user_score; //问答题得分
	
	private int num; //试卷生成序号

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}

	public String getUser_answer() {
		return user_answer;
	}

	public void setUser_answer(String user_answer) {
		this.user_answer = user_answer;
	}

	public String getUser_score() {
		return user_score;
	}

	public void setUser_score(String user_score) {
		this.user_score = user_score;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}
	
	
	
	
}
