    overflow:hidden;
}
/*分数*/
.task-score{
    width: 90px;
    height: 50px;
    background-image:url("../img/yz.png") ;
    background-size: 90px 50px;
    display: block;
    overflow: hidden;
    color: #ffffff;
    transform: rotate(-40deg);
    right: 20px;
    top: 20px;
    position: absolute;
    text-align: center;
    line-height: 50px;
    font-size: 30px;
}
.pingyu-book{
    border-radius: 5px;
    padding: 5px;
    border: 1px solid #dadada;
    display: block;
    overflow: hidden;
    background-color: #faebcc;
}
