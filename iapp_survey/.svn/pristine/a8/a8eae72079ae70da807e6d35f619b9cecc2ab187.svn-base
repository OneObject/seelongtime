package com.longtime.app.ixin.mgr.survey.model;

public class Survey {

	private String id; //主键(uuid)
	
	private String title; //名称
	
	private String cover; //封面
	
	private String remark; //简介
	
	private long weight; //排序条件
	
	private long start_time; //调研开始时间（0 表示不限制）
	
	private long end_time; //调研结束时间（0 表示不限制）
	
	private String logo; //问卷展示logo
	
	private int  show_logo; //0 不显示（默认） 1显示
	
	private int release_status; //发布状态 0 未发布 1已发布
	
	private int collect_status; //调研状态 0 初始值未手工设置 1 开始收集 2 停止收集
	
	private int page_num; //问卷分几页
	
	private int collect_num; //收集多少份试卷时结束
	
	private int type; //0 域内私有 1共享 2 官方推荐
	
	private int color; //颜色枚举
	
	private int show_progress; //多页时显示进度0 不显示，1显示（默认）
	
	private int show_title_num; //显示题目序号0 不显示，1显示（默认）
	
	private int use_pwd; //启用访问密码 0 不启用（默认） 1启用
	
	private String pwd; //访问密码（4位）
	
	private int show_person_result; //展示个人答题结果0 不展示（默认） 1展示
	
	private int show_statistics; //显示调研整体结果 0不显示（默认） 1显示
	
	private int answer_num; //答卷数量
	
	private String prompt; //结束时提示信息
	
	private String domain; //域
	
	private Long create_time; //创建时间
	
	private String creator; //uuid 创建人ID
	
	private Long update_time; //最后修改时间
	
  	private String updater; //uuid 最后修改人ID
	
	private int isdel; //是否删除 0未删除 1已删除
	
	private int once_only;//只允许答题一次 1 ，默认可以多次 0
	
	private int ip_oncepnly;//每个IP只允许答题一次 1是   0否（默认）
	
	private int weixinonly; //值允许微信答题   1  是 0否（默认）

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCover() {
		return cover;
	}

	public void setCover(String cover) {
		this.cover = cover;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public long getWeight() {
		return weight;
	}

	public void setWeight(long weight) {
		this.weight = weight;
	}

	public long getStart_time() {
		return start_time;
	}

	public void setStart_time(long start_time) {
		this.start_time = start_time;
	}

	public long getEnd_time() {
		return end_time;
	}

	public void setEnd_time(long end_time) {
		this.end_time = end_time;
	}

	public String getLogo() {
		return logo;
	}

	public void setLogo(String logo) {
		this.logo = logo;
	}

	public int getShow_logo() {
		return show_logo;
	}

	public void setShow_logo(int show_logo) {
		this.show_logo = show_logo;
	}

	public int getRelease_status() {
		return release_status;
	}

	public void setRelease_status(int release_status) {
		this.release_status = release_status;
	}

	public int getCollect_status() {
		return collect_status;
	}

	public void setCollect_status(int collect_status) {
		this.collect_status = collect_status;
	}

	public int getPage_num() {
		return page_num;
	}

	public void setPage_num(int page_num) {
		this.page_num = page_num;
	}

	public int getCollect_num() {
		return collect_num;
	}

	public void setCollect_num(int collect_num) {
		this.collect_num = collect_num;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public int getColor() {
		return color;
	}

	public void setColor(int color) {
		this.color = color;
	}

	public int getShow_progress() {
		return show_progress;
	}

	public void setShow_progress(int show_progress) {
		this.show_progress = show_progress;
	}

	public int getShow_title_num() {
		return show_title_num;
	}

	public void setShow_title_num(int show_title_num) {
		this.show_title_num = show_title_num;
	}

	public int getUse_pwd() {
		return use_pwd;
	}

	public void setUse_pwd(int use_pwd) {
		this.use_pwd = use_pwd;
	}

	public String getPwd() {
		return pwd;
	}

	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	public int getShow_person_result() {
		return show_person_result;
	}

	public void setShow_person_result(int show_person_result) {
		this.show_person_result = show_person_result;
	}

	public int getShow_statistics() {
		return show_statistics;
	}

	public void setShow_statistics(int show_statistics) {
		this.show_statistics = show_statistics;
	}

	public int getAnswer_num() {
		return answer_num;
	}

	public void setAnswer_num(int answer_num) {
		this.answer_num = answer_num;
	}

	public String getPrompt() {
		return prompt;
	}

	public void setPrompt(String prompt) {
		this.prompt = prompt;
	}

	public String getDomain() {
		return domain;
	}

	public void setDomain(String domain) {
		this.domain = domain;
	}

	public Long getCreate_time() {
		return create_time;
	}

	public void setCreate_time(Long create_time) {
		this.create_time = create_time;
	}

	public String getCreator() {
		return creator;
	}

	public void setCreator(String creator) {
		this.creator = creator;
	}

	public Long getUpdate_time() {
		return update_time;
	}

	public void setUpdate_time(Long update_time) {
		this.update_time = update_time;
	}

	public String getUpdater() {
		return updater;
	}

	public void setUpdater(String updater) {
		this.updater = updater;
	}

	public int getIsdel() {
		return isdel;
	}

	public void setIsdel(int isdel) {
		this.isdel = isdel;
	}

	@Override
	public String toString() {
		return String
				.format("Survey [id=%s, title=%s, cover=%s, remark=%s, weight=%s, start_time=%s, end_time=%s, logo=%s, show_logo=%s, release_status=%s, collect_status=%s, page_num=%s, collect_num=%s, type=%s, color=%s, show_progress=%s, show_title_num=%s, use_pwd=%s, pwd=%s, show_person_result=%s, show_statistics=%s, answer_num=%s, prompt=%s, domain=%s, create_time=%s, creator=%s, update_time=%s, updater=%s, isdel=%s]",
						id, title, cover, remark, weight, start_time, end_time,
						logo, show_logo, release_status, collect_status,
						page_num, collect_num, type, color, show_progress,
						show_title_num, use_pwd, pwd, show_person_result,
						show_statistics, answer_num, prompt, domain,
						create_time, creator, update_time, updater, isdel);
	}

	public int getOnce_only() {
		return once_only;
	}

	public void setOnce_only(int once_only) {
		this.once_only = once_only;
	}

	public int getIp_oncepnly() {
		return ip_oncepnly;
	}

	public void setIp_oncepnly(int ip_oncepnly) {
		this.ip_oncepnly = ip_oncepnly;
	}

	public int getWeixinonly() {
		return weixinonly;
	}

	public void setWeixinonly(int weixinonly) {
		this.weixinonly = weixinonly;
	}
	
}
