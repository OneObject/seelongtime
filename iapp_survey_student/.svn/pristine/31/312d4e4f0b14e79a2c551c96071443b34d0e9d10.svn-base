    padding-bottom: 10px;
}
.task-list-box ul li:hover{
    cursor: pointer;
    background-color: #eaeaea;
}
/*iconwarning:未完成   iconsuccess:通过     icondanger:未通过    iconinfo：已完成*/
.iconwarning{
    color:#F0AD4E;
}
.iconsuccess{
    color: #5CB85C;
}
.icondanger{
    color: #D9534F;
}
.iconinfo{
    color: #428BCA;
}
/*label-warning:未开始   label-success:通过     label-danger:未通过    label-info：已完成 */
