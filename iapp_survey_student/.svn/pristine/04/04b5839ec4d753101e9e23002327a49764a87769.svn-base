    private BigDecimal        completepercent; 
    
    private int               openstate = OPEN_STATE_UNOPENED;
    
    //公开
    public static int OPEN_STATE_OPENED = 1;
    //不公开
    public static int OPEN_STATE_UNOPENED = 2;

    private String            path;

    private String            remark;

    private int               score            = 0;

    private int               sort             = 0;

