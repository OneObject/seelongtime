package com.longtime.common.exception;

import org.apache.commons.lang3.exception.ExceptionUtils;

public class IAppRuntimeException extends RuntimeException{

	private static final long serialVersionUID = -6539583102529274065L;
	
	public IAppRuntimeException(){
		
	}
	
	public IAppRuntimeException(String message){
		super(message);
	}
	
	public IAppRuntimeException(Throwable cause) {
		super(ExceptionUtils.getMessage(cause), cause);
	}
	
	public IAppRuntimeException(String message, Throwable cause) {
		super(message, cause);
	}

}
