html,body,div,span,a,p,section,article,dl,ul,dd,dt,li,img{
    margin:0;padding:0;border:0;
}
dl,ul{
    list-style: none;
}
a,button,input,textarea,span,img,li,div{
    -webkit-tap-highlight-color:rgba(255,0,0,0);
}
section,div,a,ul,dl,span,p,dd,dt,li,label{
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
body{
	font-family: "microsoft yahei", "sans-serif";
	font-size:14px;	
    min-width: 320px;
    color:rgba(0,0,0,0.87);
    -ms-touch-action:none;
    background: #eee;
}
input[type="search"] {
    -webkit-appearance: none;
}
a{
    color:rgba(0,0,0,0.87);
    text-decoration: none;
    cursor: pointer;
}
section.banner{
	position:relative;
}
.swiper-wrapper img{
    display: block;
    width:100%;
}
.pagination {
	position: absolute;
	left: 0;
	text-align: center;
	bottom: 5px;
	width: 100%;
	z-index: 100;
}
.swiper-pagination-switch {
	display: inline-block;
	width: 8px;
	height: 8px;
	border-radius: 8px;
	background: rgba(255, 255, 255, 0.48);
	margin: 0 3px;
	cursor: pointer;
}
.swiper-active-switch {
	background: rgba(255, 255, 255, 1);
}
.head-line{
	width:100%;
	height:34px;
	line-height: 34px;
	font-size:16px;
	padding:0 3%;
	color:rgba(0,0,0,0.48);
	overflow: hidden;
	font-weight: bold;
}
.head-line a{
	float:right;
	text-decoration: underline;
	color:#529bea;	
}
.ctl-margin-bottom{
	margin-bottom:0 !important;
}
.ctl-margin-bottom a{
	margin-bottom:0 !important;
}
.show-model-1 a{
	display: inline-block;
	float:left;
	width:29%;
	background: #fff;
	margin-bottom: 3%;
}
.show-model-1 a:nth-child(3n+1){
	margin-left:3%;
}
.show-model-1 a:nth-child(3n+2){
	margin-left:3%;
	margin-right:3%;
	width:30%;
}
.show-model-1 a:nth-child(3n+3){
	margin-right:3%;
}
.show-model-1 a div{
	width:100%;
	height:72px;
	padding:8px;
	margin: 0 auto;
}
.show-model-1 div span{
	display: inline-block;
	width:100%;
	height:34px;
	float:left;
}
.show-model-1 span img{
	display: block;
	width:34px;
	height:34px;
	float:left;
}
.show-model-1 div label{
	display: inline-block;
	width:100%;
	height:16px;
	line-height: 16px;
	font-size:14px;
	text-align: right;
	float:left;
	margin-top:6px;
	color:rgba(0,0,0,0.48);
}
.show-model-2 a{
